{
  "name": "@mono/fns",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "packageManager": "npm@10.2.3",
  "files": [
    "dist",
    ".env.*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsup-node",
    "dev": "concurrently \"npm:build:watch\" \"npm:emulate\"",
    "lint": "eslint \"**/*.ts*\"",
    "deploy": "firebase deploy",
    "emulate": "turbo build && isolate && firebase emulators:start --project demo-mono-ts",
    "db:get-indexes": "firebase firestore:indexes > firestore.indexes.json"
  },
  "license": "MIT",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@mono/backend": "*",
    "@mono/common": "*",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11",
    "firebase-functions": "^4.5.0",
    "firestore-server-utils": "1.0.0-4",
    "lodash-es": "^4.17.21",
    "nocache": "^4.0.0",
    "ts-is-present": "^1.2.2"
  },
  "devDependencies": {
    "@mono/eslint-config-custom": "*",
    "@mono/tsconfig": "*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "firebase-tools": "13.0.2",
    "isolate-package": "1.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
